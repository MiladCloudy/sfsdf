  top: 56px;
  right: 23px;
  z-index: 1000;
  box-shadow: 0 0 5px @cartPerviewHolder-boxShadow-Color;
  padding: 10px;
  border-radius: 10px 0px 10px 10px;
  text-align: center;

  &:before {
    border-bottom: 14px solid @cartPerviewHolder-bgColor;
    border-left: 19px solid transparent;
    //border-right: 10px solid transparent;
    content: "";
    height: 0;
    right: -116px;
    position: relative;
    top: -40px;
    width: 0;
  }
}

.cartPerviewTotal {
  margin-top: 10px;
  line-height: 30px;
}

.viewTypeBtn {
