//-- cartPerviewPanel
.cartPerviewPanel {
  * {
   // border: 1px solid red !important;
  }

  display: flex;
  vertical-align: middle;
  flex-direction: row-reverse;
  align-items: center;
  transition: all 0.3s;
  padding: 8px;
  border: 1px solid @cartPerviewPanel-borderColor;
  margin-bottom: 5px;

  &:hover {
    transition: all 0.3s;
    box-shadow: 0 0 5px @cartPerview-boxShadow-Color;
  }
}

.cartPerviewPanelRightSide {
  display: inline-flex;
  flex-direction: column;
  margin-left: 5px;
  width: 100%;
}

.cartPerviewProductName {
  display: block;
  text-align: left;
  font-size: 90%;
  color: @productPanel-color;
}

.cartPerviewCurrentPrice, .cartPerviewQuantity {
  text-align: left;
  color: @currentPrice-color;
  font-size: 90%;
  display: inline-block;
}

.cartPerviewQuantity {
  color: @quantity-color;
}

.cartPerviewProductThumbWrapper {
  border: 1px solid @productPanel-borderColor;
  transition: box-shadow 0.3s;
  width: 64px;
  height: 64px;
  margin-top: 10px;
  display: inline-flex;
  align-items: center;
  justify-items: center;
  justify-content: center;
  flex-direction: column;

  &:hover {
    box-shadow: 0px 0px 10px @productPanel-shadowColor;
    transition: box-shadow 0.3s;
  }
}

.cartPerviewProductThumb {
  width: 64px;
}

.cartPerviewRemoveBtn {
  transition: all 0.3s;
  color: @removeBtn-color;
  border: 1px solid @removeBtn-borderColor;
  border-radius: 50%;
  font-size: 6pt;
  padding: 4px;
  display: inline-flex;
  width: 6px;
  height: 6px;
  text-align: center;
  vertical-align: middle;
  line-height: 6pt;
  justify-content: center;
  align-items: center;
  cursor: pointer;
  margin-left: 10px;

  &:hover {
    transition: all 0.3s;
    color: @removeBtn-hover-color;
    border: 1px solid @removeBtn-hover-borderColor;
    border-radius: 30%;
    font-size: 5pt;
  }
}

#cartPerviewHolder {
  width: 250px;
