//11ssaaa
@import "base.less";
@import "elements.less";
@import "font.less";
@import "icon.less";
@import "color.less";
@import "animate.less";

//!-style

img {
  vertical-align: middle;
}


* {
  font-family: nassim;
  /*font-family: IRANSans;*/
  margin: 0;
  padding: 0;

}

html {
  //background-color: #222;
}

body {
  //background-color: #222;
  background: url("/image/bg/bgs3.png") repeat;
  height: 100%;
  color: #fff;
  font-size: 12pt;
  line-height: 15px;
}

html, body {
  direction: rtl;
}

textarea, input, select, option {
  border: 1px solid #8a8;
  background-color: #152;
  color: #fff;
  min-height: 30px;
  max-height: 30px;
  padding: 5px;
  font-size: 10pt;
  line-height: 10pt;
  box-sizing: border-box;
  vertical-align: middle;
  font-family: nassim;
  margin: 0px 5px;

  &:hover {
    border: 1px solid #ccc;
  }

  &:focus {
    border: 1px solid #aaf;
  }

  .placeholder({
    font-family: nassim;
  });

  &.itemQuantity {  }
}

input, textarea {
  border: 1px solid #464646;
  box-shadow: 2px 2px 2px rgba(34,34,34,0.3);
  font-size: 12pt;
  height: 46px;
  color: #ccc;
}

input[type=number] {
  -moz-appearance:textfield;
}

textarea {
  max-height: 200px !important;
  height: 150px !important;
}

select {
  font-family: nassim;
  width: 100px;
}

a, a:visited, a:hover {
  text-decoration: none;
}

a, a:link {
  color: @link-color;
}

a:hover {
  color: @link-hover-color;
}

span:not(a) {
  //color: white;
}

.en, .ltr {
  font-family: tahoma;
}

.wrong {
  &:extend(input);
  border: 1px solid rgba(127,0,0,0.3);
  background-color: rgba(127,70,70,0.2);
  box-shadow: 0 0 5px
  #000 inset;
  padding: 10px;
  min-height: 46px;
  max-height: 46px;

  &:hover {
    border: 1px solid rgba(127,0,0,0.8) !important;
  }

  &:focus {
    border: 1px solid rgba(127,0,0,0.8) !important;
  }
}

.correct {
  &:extend(input);
  border: 1px solid rgba(0, 127, 0, 0.3);
  background-color: rgba(70, 127, 70, 0.2);
  box-shadow: 0 0 5px
  #000 inset;
  padding: 10px;
  min-height: 46px;
  max-height: 46px;

  &:hover {
    border: 1px solid rgba(0, 127, 0, 0.8) !important;
  }

  &:focus {
    border: 1px solid rgba(70, 127, 0, 0.8) !important;
  }
}

.wrong1 {
  &:extend(input);
  border: 1px solid rgba(255, 127, 0, 0.3);
  background-color: rgba(255, 127, 0, 0.2);
  box-shadow: 0 0 5px
  #000 inset;
  padding: 10px;
  min-height: 46px;
  max-height: 46px;

  &:hover {
    border: 1px solid rgba(255,127,0,0.8) !important;
  }

  &:focus {
    border: 1px solid rgba(255,127,0,0.8) !important;
  }
}


#content {
  max-width: 95%;
  margin: 70px auto 70px auto;
}

#header-wrapper, #footer-wrapper {
  background-color: #223;
  position: fixed;
  width: 100%;
  height: 40px;
  border: none;
  z-index: 1000;
}

#header-wrapper {
  top: 0;
  border-bottom: 1px solid #22a;
  padding: 5px 8px;
}

#footer-wrapper {
  bottom: 0;
  border-top: 1px solid #22a;
}

.productPanel {
  position: relative;
  width: 274px;
  height: 350px;
  border: 1px solid @productPanel-borderColor;
  background-color: @productPanel-bg;
  transition: box-shadow 0.3s;
  text-align: center;
  overflow: hidden;
  display: inline-block;
  margin: 5px;

  &:hover {
    box-shadow: 0px 0px 10px @productPanel-shadowColor;
    transition: box-shadow 0.3s;

    .wishBtn {
      margin: 0px;
      transition: all 0.2s;
    }
  }
}

.proceedBtn, .addToCartBtn, .sbtn, .btn, .navActiveBtn, .navNoActiveBtn {
  display: inline-block;
  padding: 0px 4px;
  background-color: @addToCartBtn-bg;
  color: @addToCartBtn-color;
  border: none;
  border-bottom: 2px solid @addToCartBtn-borderColor;
  cursor: pointer;
  border-radius: 4px;
  transition: all 0.2s;
  vertical-align: middle;
  text-decoration: none;
  font-size: 10pt;

  &:hover {
    background-color: @addToCartBtn-hover-bg;
    color: @addToCartBtn-hover-color;
    border-bottom: 2px solid @addToCartBtn-hover-borderColor;
    transition: all 0.2s;

    &:extend(a);
    color: @addToCartBtn-hover-color !important;
  }

  span:before {
    font-size: 11px;
    line-height: 26px;
    vertical-align: middle;
  }

  &:extend(a);
  color: @addToCartBtn-color !important;
}

.addToCartBtn {
  position: absolute;
  bottom: 10px;
  width: 90%;
  left: 0;
  right: 0;
  margin: auto;
  align-items: center;
  text-align: center;
}

.proceedBtn {
  width: 100px;
  bottom: 10px;
  left: 0;
  right: 0;
  margin: auto;
  align-items: center;
  text-align: center;
}

.sbtn {
  background-color: @notFound-bg;
  color: #fff !important;
  border: 1px solid #fff;
  /*border: inherit;*/
  padding: 5px 12px;
  cursor: pointer;
  font-size: 10pt;
  text-decoration: none;
  border-radius: 2px;
  transition: all 0.2s;

  -webkit-box-shadow: 0 2px 5px rgba(0,0,0,.1);
  box-shadow: 0 2px 5px rgba(0,0,0,.1);

  &:extend(a);
  color: #fff !important;

  &:hover {
    background-color: #fff !important;
    color: @notFound-bg;
    border: 1px solid @notFound-bg;
    transition: all 0.2s;
  }
}

.navActiveBtn {
  background-color: @notFound-bg;
  color: #fff !important;
  border: 1px solid #fff;
  /*border: inherit;*/
  padding: 7px 14px;
  cursor: pointer;
  font-size: 10pt;
  text-decoration: none;
  margin-top: 9px;
  border-radius: 2px;
  transition: all 0.2s;

  -webkit-box-shadow: 0 2px 5px rgba(0,0,0,.1);
  box-shadow: 0 2px 5px rgba(0,0,0,.1);

  &:hover {
    background-color: #fff !important;
    color: @notFound-bg;
    border: 1px solid @notFound-bg;
    font-size: 9pt;
    transition: all 0.2s;
  }
}

.navNoActiveBtn {
  background-color: #2ecc71;
  color:  @addToCartBtn-color;
  border: 1px solid #0a7;
  /*border: inherit;*/
  padding: 7px 14px;
  cursor: pointer;
  font-size: 10pt;
  text-decoration: none;
  margin-top: 9px;
  border-radius: 2px;
  transition: all 0.2s;

  -webkit-box-shadow: 0 2px 5px rgba(0,0,0,.1);
  box-shadow: 0 2px 5px rgba(0,0,0,.1);

  &:hover {
    background-color: @addToCartBtn-color;
    color: #0abe57 !important;
    border: 1px solid #0a7;
    font-size: 9pt;
    transition: all 0.2s;
  }
}

.btn {
  background-color: #2ecc71 !important;
  color: @addToCartBtn-color !important;
  border: 1px solid #0a7 !important;
  /*border: inherit;*/
  padding: 5px 10px;
  cursor: pointer;
  font-size: 10pt;
  text-decoration: none;
  margin-top: 9px;
  border-radius: 2px;
  transition: all 0.2s;

  -webkit-box-shadow: 0 2px 5px rgba(0,0,0,.1);
  box-shadow: 0 2px 5px rgba(0,0,0,.1);

  &:extend(a);
  color: @addToCartBtn-color !important;

  &:hover {
    background-color: @addToCartBtn-color !important;
    color: #0abe57 !important;
    font-size: 9pt;
    transition: all 0.2s;
    /*border: 1px solid #223;
    box-shadow: 0px 1px 1px #ddd;*/

    &:extend(a);
    color: #2ecc71 !important;
  }
}

.productName {
  margin-top: 10px;
  display: block;
  color: @productPanel-color;
  font-size: 110%;
}

.priceWrapper {
  * {
    //font-size: 120%;
  }

  direction: ltr;
}

.currentPrice {
  color: @currentPrice-color;
  font-size: 140%;
  margin-left: 5px;
}

.oldPrice {
  color: @oldPrice-color;
  font-size: 100%;
  text-decoration: line-through;
}

.saleFlag {
  position: absolute;
  left: 0;
  top: 20px;
  padding: 1px 4px;
  background-color: @saleBtn-bg;
  color: @saleBtn-color;
  border-radius: 0px 4px 4px 0px;
  border: 1px solid transparent;
}

.discountFlag {
  position: absolute;
  left: 0;
  top: 20px;
  padding: 1px 4px;
  background-color: @discountFlag-bg;
  color: @discountFlag-color;
  border-radius: 0px 4px 4px 0px;
  border: 1px solid transparent;
}

.notFound {
  margin-top: 60px;
  padding: 5px 10px;
  background-color: @notFound-bg;
  color: @notFound-color;
  border-radius: 0px 4px 4px 0px;
  border: 1px solid transparent;
}

.wishBtn {
  position: absolute;
  right: 0;
  top: 20px;
  margin: 0 -25px;
  padding: 2px 4px 4px 4px;
  background-color: @wishBtn-bg;
  color: @wishBtn-color;
  border-radius: 4px 0px 0px 4px;
  border: 1px solid transparent;
  cursor: pointer;
  transition: all 0.2s;

  &:hover {
    background-color: @wishBtn-hover-bg;
    transition: all 0.2s;
  }
}

.productThumbWrapper {
  height: 200px;
  margin-top: 10px;
  display: flex;
  align-items: center;
  justify-content: space-around;
}

.productThumb {
  width: 192px;
  margin-top: 20px;
  display: inline-block;
}


